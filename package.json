{
  "name": "js-bp",
  "version": "0.1.0",
  "description": "JavaScript Types",
  "license": "MIT",
  "repository": "github.com/ccorcos/js-type",
  "author": {
    "name": "Chet Corcos",
    "email": "ccorcos@gmail.com",
    "url": "chetcorcos.com"
  },
  "main": "src/type.js",
  "scripts": {
    "test": "xo && ava"
  },
  "xo": {
    "space": 2,
    "semicolon": false,
    "rules": {
      "no-new-func": 0,
      "guard-for-in": 0,
      "new-cap": 0,
      "no-use-before-define": 0,
      "camelcase": 0
    }
  },
  "keywords": [
    "type",
    "class",
    "typeclass",
    "functional",
    "functor",
    "monad"
  ],
  "dependencies": {
    "is-js": "^0.1.1",
    "ramda": "^0.21.0"
  },
  "devDependencies": {
    "ava": "*",
    "xo": "*"
  }
}
